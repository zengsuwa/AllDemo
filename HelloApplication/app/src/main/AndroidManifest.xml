<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zsw.zeng.helloapplication">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/car_tab_red"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".noslide.NoSlideActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".slide.SlideActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".status.StatusActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".status.SystemStatusActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".status.SecondStatusActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".status.ThirdStatusActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".mvp.MvpActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".banner.BannerActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity android:name=".SelfViewActivity"></activity>
        <activity android:name=".fragmentlearn.LearnActivity"></activity>
        <activity android:name=".coordinator.CoordinatorActivity"></activity>
        <activity android:name=".activity.SurfaceActivity"></activity>
        <activity android:name=".activity.AnimationActivity"></activity>
        <activity android:name=".activity.ApkInfoActivity"></activity>
        <activity android:name=".activity.AMProcessActivity"></activity>
        <activity
            android:name=".activity.SchemeActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="andy" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ServiceTestActivity"></activity>
        <activity android:name=".activity.ActionActivity" />

        <service android:name=".service.StartService" />
        <service android:name=".service.BindService" />

        <activity android:name=".activity.PickerViewActivity"></activity>
    </application>

</manifest>